<?xml version="1.0"?>

<!DOCTYPE circle
[
   <!ELEMENT circle (position)>
   <!ELEMENT position EMPTY>
   <!ATTLIST circle
             radius CDATA #REQUIRED
   >
   <!ATTLIST position
             x CDATA #REQUIRED
             y CDATA #REQUIRED
   >
]>

<circle radius="15">
  <position x="30" y="50"/>
</circle>